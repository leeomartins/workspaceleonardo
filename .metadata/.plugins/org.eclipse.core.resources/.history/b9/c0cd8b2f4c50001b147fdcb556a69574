package repeticao;

public class ExemploFor {

	public static void main(String[] args) {
		/*
		 * 
		 * Sintaxe do For
		 * 1 Parte: definir o contador e valor inicial
		 * 2 Parte: condição (enquanto for verdadeiro o laço irá continuar)
		 * 3 Parte: como ocorrerá a cotagem
		 * 
		 * 
		 */

		
	for (int contador=0;contador<500;contador++) {
		System.out.println(contador);
	}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
